{
    "_comment": "You can add multiple folders to model_paths. Environment variables are expanded, and relative paths are allowed. If the same model is found in multiple folders, the first one will be used.",
    "resource_paths": [
	"./components",
	"$HOME/.local/share/ws_textproc"
    ],
    "textprocs": [
	{
	    "name": "sv_se_1",
	    "lang": "sv",
	    "rbnf_lang": "sv",
	    "enabled": true,
	    "load_on_startup": false,
	    "token_split_re": "\\s+",
	    ".sentence_split_re": "[\\p{Ll}0-9][”\")]?[.!?]+[”\")]?(\\s+)[\"]?\\p{Lu}",
	    "sentence_split_re": "[.?!] [A-Z0-9]+",
	    "expansion_dicts": [
		"sv_abbrevs.txt",
		"sv_numerals.txt"
		]
	}
    ]
}
